<!-- BEGIN CONTENT -->
<div class="page-content-wrapper">
    <!-- BEGIN CONTENT BODY -->
    <div class="page-content">
        <!-- BEGIN PAGE HEADER-->
        <!-- BEGIN PAGE BAR -->
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li>
                    <a href="\Comparacao\ConferenciaLista">Conferencia</a>
                    <i class="fa fa-circle"></i>
                </li>
                <li>
                    <a href="\Comparacao\PassagensPedagioLista">Passagens Pedagio </a>
                    <i class="fa fa-circle"></i>
                </li>
                <li>
                    <a href="\Comparacao\PassagensValePedagioLista">Passagens Vale Pedagio </a>
                    <i class="fa fa-circle"></i>
                </li>
                <li>
                    <a href="\Comparacao\CreditosLista">Creditos </a>
                </li>
            </ul>

        </div>
        <!-- END PAGE BAR -->
        <!-- BEGIN PAGE TITLE-->
        <h3 class="page-title">
            Passagens Pedagio
        </h3>
        <!-- END PAGE TITLE-->
        <!-- END PAGE HEADER-->


        <div class="row">
            <div class="col-md-10">
                <form asp-controller="Comparacao" asp-action="PassagensPedagioFiltroLista">
                    <div class="form-body">
                        <div class="form-group">

                            <div class="col-md-3">
                                Data Inicio
                                <input class="form-control input-small mask_date" placeholder="Data Inicio" name="dataInicio" type="text" />
                            </div>
                            <div class="col-md-3">
                                Data Fim
                                <input class="form-control input-small mask_date" placeholder="Data Fim" name="dataFim" type="text" />
                            </div>

                            <div class="col-md-3">
                                Placa
                                <input class="form-control input-small " placeholder="Placa" name="placa" type="text" />
                            </div>

                            <div class="col-md-3">
                                <br />
                                <button type="submit" class="btn blue">
                                    <i class="fa fa-check"></i> Pesquisar
                                </button>
                            </div>

                        </div>
                    </div>
                </form>
            </div>
        </div>


        <div class="row">
            <div class="col-md-12">



                <!-- BEGIN EXAMPLE TABLE PORTLET  INICIO CONTEUDO TABELA-->
                <div class="portlet-body">
                    <div class="table-toolbar">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="btn-group">

                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="btn-group pull-right">
                                    <button class="btn green  btn-outline dropdown-toggle" data-toggle="dropdown">
                                        Exportar
                                        <i class="fa fa-angle-down"></i>
                                    </button>
                                    <ul class="dropdown-menu pull-right">
                                        <li>
                                            <a href="javascript:;">
                                                <i class="fa fa-file-pdf-o"></i> PDF
                                            </a>
                                        </li>
                                        <li>
                                            <a href="javascript:;">
                                                <i class="fa fa-file-excel-o"></i> Excel
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                    <table class="table table-striped table-bordered table-hover table-checkable order-column" id="sample_1">
                        <thead>
                            <tr>
                                <th> Placa </th>
                                <th> Tag </th>
                                <th> Prefixo </th>
                                <th> Marca </th>
                                <th> Categ </th>
                                <th> Data </th>
                                <th> Hora </th>
                                <th> Rodovia </th>
                                <th> Praca </th>
                                <th> Valor </th>
                            </tr>
                        </thead>
                        <tbody>

                            @foreach (var item in Model)
                            {
                                <tr class="odd gradeX">

                                    <td>
                                        @item.Placa
                                    </td>
                                    <td>
                                        @item.Tag
                                    </td>
                                    <td>
                                        @item.Prefixo
                                    </td>
                                    <td>
                                        @item.Marca
                                    </td>
                                    <td>
                                        @item.Categ
                                    </td>
                                    <td>
                                        @item.Data.ToString("dd/MM/yyyy")
                                    </td>
                                    <td>
                                        @item.Hora
                                    </td>
                                    <td>
                                        @item.Rodovia
                                    </td>
                                    <td>
                                        @item.Praca
                                    </td>
                                    <td>
                                        @item.Valor
                                    </td>

                                </tr>
                            }

                        </tbody>
                    </table>
                </div>
            </div>
            <!-- END EXAMPLE TABLE PORTLET-->




        </div>
    </div>

</div>
<!-- END CONTENT BODY -->
        </div>
<!-- END CONTENT -->
